{
  "name": "gabi-family-docs-app",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "detox test --configuration ios.sim.release",
    "test:e2e:android": "detox test --configuration android.emu.release",
    "test:e2e:debug": "detox test --configuration ios.sim.debug",
    "build:ios": "detox build --configuration ios.sim.release",
    "build:android": "detox build --configuration android.emu.release",
    "build:ios:debug": "detox build --configuration ios.sim.debug",
    "build:android:debug": "detox build --configuration android.emu.debug",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "security:test": "npm audit && npm audit fix",
    "performance:test": "npx react-native-performance-test",
    "accessibility:test": "npx react-native-accessibility-test",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-community/netinfo": "^11.1.0",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.7",
    "@shopify/react-native-performance": "^1.0.0",
    "@supabase/supabase-js": "^2.56.0",
    "expo": "~53.0.22",
    "expo-document-picker": "^13.1.6",
    "expo-file-system": "^18.1.11",
    "expo-image-manipulator": "^12.0.0",
    "expo-image-picker": "^16.1.4",
    "expo-notifications": "^0.31.4",
    "expo-secure-store": "^14.2.3",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-native": "0.79.6",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "^3.6.0",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-svg": "^14.1.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-modal": "^13.0.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-super-grid": "^4.8.1",
    "react-native-snap-carousel": "^4.0.0-beta.6",
    "react-native-chart-kit": "^6.12.0",
    "react-native-svg-charts": "^5.4.0",
    "react-native-pdf": "^6.7.4",
    "react-native-fs": "^2.20.0",
    "react-native-share": "^10.0.2",
    "react-native-camera": "^4.2.1",
    "react-native-vision-camera": "^3.9.0",
    "react-native-permissions": "^4.1.1",
    "react-native-device-info": "^10.12.0",
    "react-native-bootsplash": "^5.1.1",
    "react-native-splash-screen": "^3.3.0",
    "react-native-orientation-locker": "^1.5.0",
    "react-native-background-timer": "^2.4.1",
    "react-native-background-job": "^2.2.0",
    "react-native-background-fetch": "^4.2.1",
    "react-native-background-upload": "^6.3.0",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-biometrics": "^3.0.1",
    "react-native-keychain": "^8.1.3",
    "react-native-crypto-js": "^1.0.0",
    "react-native-aes-crypto": "^1.0.0",
    "react-native-randombytes": "^3.6.1",
    "react-native-hash": "^1.0.0",
    "react-native-md5": "^1.0.0",
    "react-native-sha256": "^1.0.0",
    "react-native-sha1": "^1.0.0",
    "react-native-base64": "^0.2.1",
    "react-native-qrcode-scanner": "^1.5.5",
    "react-native-qrcode-svg": "^6.3.0",
    "react-native-barcode-scanner-ios": "^1.0.0",
    "react-native-signature-canvas": "^4.0.0",
    "react-native-signature-pad": "^0.0.8",
    "react-native-signature-capture": "^0.4.12",
    "react-native-signature-canvas": "^4.0.0",
    "react-native-signature-pad": "^0.0.8",
    "react-native-signature-capture": "^0.4.12",
    "react-native-signature-canvas": "^4.0.0",
    "react-native-signature-pad": "^0.0.8",
    "react-native-signature-capture": "^0.4.12"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "@babel/preset-typescript": "^7.23.0",
    "@detox/runners/jest": "^20.0.0",
    "@jest/globals": "^29.7.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-native": "^5.4.0",
    "@testing-library/react-native": "^12.4.0",
    "@types/jest": "^29.5.0",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-jest": "^29.7.0",
    "detox": "^20.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "react-test-renderer": "^18.2.0",
    "typescript": "~5.8.3"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.js"],
    "transformIgnorePatterns": [
      "node_modules/(?!((@)?react-native|@shopify|react-native-reanimated|react-native-gesture-handler|react-native-svg|react-native-linear-gradient|react-native-modal|react-native-keyboard-aware-scroll-view|react-native-super-grid|react-native-snap-carousel|react-native-chart-kit|react-native-svg-charts|react-native-pdf|react-native-fs|react-native-share|react-native-camera|react-native-vision-camera|react-native-permissions|react-native-device-info|react-native-bootsplash|react-native-splash-screen|react-native-orientation-locker|react-native-background-timer|react-native-background-job|react-native-background-fetch|react-native-background-upload|react-native-encrypted-storage|react-native-biometrics|react-native-keychain|react-native-crypto-js|react-native-aes-crypto|react-native-randombytes|react-native-hash|react-native-md5|react-native-sha256|react-native-sha1|react-native-base64|react-native-qrcode-scanner|react-native-qrcode-svg|react-native-barcode-scanner-ios|react-native-signature-canvas|react-native-signature-pad|react-native-signature-capture)/)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@components/(.*)$": "<rootDir>/src/components/$1",
      "^@screens/(.*)$": "<rootDir>/src/screens/$1",
      "^@services/(.*)$": "<rootDir>/src/services/$1",
      "^@utils/(.*)$": "<rootDir>/src/utils/$1",
      "^@types/(.*)$": "<rootDir>/src/types/$1",
      "^@contexts/(.*)$": "<rootDir>/src/contexts/$1",
      "^@lib/(.*)$": "<rootDir>/src/lib/$1"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.stories.{js,jsx,ts,tsx}",
      "!src/**/index.{js,jsx,ts,tsx}",
      "!src/**/*.test.{js,jsx,ts,tsx}",
      "!src/**/*.spec.{js,jsx,ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    },
    "testEnvironment": "node",
    "testTimeout": 30000,
    "verbose": true
  },
  "detox": {
    "testRunner": "jest",
    "runnerConfig": "e2e/config.json",
    "configurations": {
      "ios.sim.debug": {
        "type": "ios.simulator",
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/GabiFamilyDocs.app",
        "build": "xcodebuild -workspace ios/GabiFamilyDocs.xcworkspace -scheme GabiFamilyDocs -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "device": {
          "type": "iPhone 14"
        }
      },
      "ios.sim.release": {
        "type": "ios.simulator",
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/GabiFamilyDocs.app",
        "build": "xcodebuild -workspace ios/GabiFamilyDocs.xcworkspace -scheme GabiFamilyDocs -configuration Release -sdk iphonesimulator -derivedDataPath ios/build",
        "device": {
          "type": "iPhone 14"
        }
      },
      "android.emu.debug": {
        "type": "android.emulator",
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug",
        "device": {
          "avdName": "Pixel_4_API_30"
        }
      },
      "android.emu.release": {
        "type": "android.emulator",
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "cd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release",
        "device": {
          "avdName": "Pixel_4_API_30"
        }
      }
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:coverage && npm run type-check"
    }
  },
  "private": true
}
